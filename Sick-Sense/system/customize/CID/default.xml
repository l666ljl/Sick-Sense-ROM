<?xml version="1.0" encoding="utf-8"?>
<customization_form>
  <category name="system">
    <module name="locale">
      <function>
        <set name="single">
          <item name="total_list">en_US;es_US;</item>
          <item type="boolean" name="en_US">yes</item>
          <item type="boolean" name="es_US">yes</item>
        </set>
      </function>
    </module>
    <module name="Settings">
      <function name="screen_timeout">
        <set name="plenty">
          <item name="value1">15000</item>
          <item name="value2">30000</item>
          <item name="value3">60000</item>
          <item name="value4">120000</item>
          <item name="value5">600000</item>
          <item name="value6">-1</item>
        </set>
      </function>
    </module>
    <module name="InputMethod">
      <function name="IME_language_set">
        <set name="single">
          <item type="boolean" name="English">yes</item>
          <item type="boolean" name="German">no</item>
          <item type="boolean" name="France">no</item>
          <item type="boolean" name="Italian">no</item>
          <item type="boolean" name="Spanish">yes</item>
          <item type="boolean" name="Russian">no</item>
          <item type="boolean" name="Czech">no</item>
          <item type="boolean" name="Danish">no</item>
          <item type="boolean" name="Swedish">no</item>
          <item type="boolean" name="Norwegian">no</item>
          <item type="boolean" name="Dutch">no</item>
          <item type="boolean" name="Polish">no</item>
          <item type="boolean" name="Portugal">no</item>
          <item type="boolean" name="Greek">no</item>
          <item type="boolean" name="Turkish">no</item>
          <item type="boolean" name="Finnish">no</item>
          <item type="boolean" name="Hungarian">no</item>
          <item type="boolean" name="Romanian">no</item>
          <item type="boolean" name="Slovak">no</item>
          <item type="boolean" name="Croatian">no</item>
          <item type="boolean" name="Slovenian">no</item>
          <item type="boolean" name="Serbian">no</item>
          <item type="boolean" name="Bulgarian">no</item>
        </set>
      </function>
      <function name="IME_sip_set">
        <set name="single">
          <item name="total_list">Handwriting;ZuhYin;CangJie;PinYin;Stroke;</item>
          <item type="boolean" name="ZuhYin">yes</item>
          <item type="boolean" name="CangJie">yes</item>
          <item type="boolean" name="PinYin">yes</item>
          <item type="boolean" name="Stroke">yes</item>
        </set>
      </function>
    </module>
    <module name="deviceData">
      <function>
        <set name="single">
          <item name="device_name">PG86100</item>
        </set>
      </function>
    </module>
    <module name="deviceData1">
      <function>
        <set name="single">
          <item name="sw_number">NA</item>
        </set>
      </function>
    </module>
  </category>
  <category name="application">
    <module name="Crash_Report">
      <function name="setting">
        <set name="single">
          <item name="turn_off_report" type="boolean">yes</item>
          <item name="turn_off_tell_htc" type="boolean">yes</item>
          <item name="only_wifi_option" type="boolean">yes</item>
          <item name="hide_usage_report" type="boolean">no</item>
        </set>
      </function>
    </module>
    <module name="OOBE">
      <function name="Step_Config">
        <set name="single">
          <item name="Set_Language" type="boolean">no</item>
          <item name="Internet_Connection" type="boolean">no</item>
        </set>
      </function>
    </module>
   </category>
  <BootConfiguration>
    <BootAnimation animation="/system/customize/resource/Sprint_bootanimation.zip" />
  </BootConfiguration>
  <ShutdownConfiguration>
    <ShutdownAnimation image_png="/system/customize/resource/Sprint_downanimation.zip" image="" fps="15" />
  </ShutdownConfiguration>
  <category name="SettingsProvider">
    <module name="res">
      <function name="values">
        <set name="single">
          <item name="def_inactivity_time">0</item>
        </set>
      </function>
    </module>
  </category>
  <category name="Settings">
    <module name="properties">
      <function>
        <set name="single">
          <item name="market_name">HTC EVO 3D</item>
        </set>
      </function>
    </module>
  </category>
</customization_form>